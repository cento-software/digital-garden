---
import { type CollectionEntry, z } from "astro:content";
import BlogPost from "./BlogPost.astro";

type Props = {
    frontmatter: CollectionEntry<"writings">["data"];
};
---

<BlogPost
    {...Astro.props.frontmatter}
    pubDate={z.coerce.date().parse(Astro.props.frontmatter.pubDate)}
>
    <slot />
</BlogPost>
